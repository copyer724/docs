#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/copyer/Documents/copyer_code/note/copyer_notes/node_modules/.pnpm/esbuild@0.19.11/node_modules/esbuild/bin/node_modules:/Users/copyer/Documents/copyer_code/note/copyer_notes/node_modules/.pnpm/esbuild@0.19.11/node_modules/esbuild/node_modules:/Users/copyer/Documents/copyer_code/note/copyer_notes/node_modules/.pnpm/esbuild@0.19.11/node_modules:/Users/copyer/Documents/copyer_code/note/copyer_notes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/copyer/Documents/copyer_code/note/copyer_notes/node_modules/.pnpm/esbuild@0.19.11/node_modules/esbuild/bin/node_modules:/Users/copyer/Documents/copyer_code/note/copyer_notes/node_modules/.pnpm/esbuild@0.19.11/node_modules/esbuild/node_modules:/Users/copyer/Documents/copyer_code/note/copyer_notes/node_modules/.pnpm/esbuild@0.19.11/node_modules:/Users/copyer/Documents/copyer_code/note/copyer_notes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
